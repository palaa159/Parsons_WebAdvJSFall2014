var emptyArray = [];

//Adding items using .push()
emptyArray.push('item 01');
emptyArray.push('item 02');
emptyArray.push('12345');
emptyArray.push(['also an array']);

emptyArray.splice(0,1);
console.log(emptyArray);

var indexToRemove = emptyArray.indexOf('item 02');
console.log(emptyArray);

//removing item 02
emptyArray.splice(indexToRemove,1);
console.log(emptyArray);

//we can remove an item with the index number. but if we don't really know the index of that data, then we can find it
//through .index of. you can content of the ite,. in this case 


