var human = function(name, color, weight, height) {
	//head, body, color, weight, height

	this.name = name;
	this.color = color;
	this.weight = weight;
	this.height = height;
	this.walk = function() {
		console.log (this.name + ' is walking');
	};

};

var firstHuman = new Human('Apon', 'Purple', 65, 175);
var secondHuman = new Human('Crystal', 'Yellow', 90, 150);

secondHuman.walk();














